<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="/images/favicon.ico">

    <title>Cyclists</title>

    <!-- Bootstrap core CSS -->
    <link href="/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="/stylesheets/signin.css" rel="stylesheet">
  </head>
  <body class="text-center">
    <form action="/signin" method="POST" class="form-signin">
      <a href="/">
        <img class="mb-4" src="/images/favicon.ico" alt="" width="72" height="72">
      </a>
      <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1>
      <label for="inputID" class="sr-only">Email address</label>
      <input type="id" id="inputID" name="id" class="form-control" placeholder="Type your ID" required autofocus>
      <label for="inputPassword" class="sr-only">Password</label>
      <input type="password" id="inputPassword" name="password" class="form-control" placeholder="Password" required>
      <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
      <p class="mt-5 mb-3 text-muted">If your not registered yet, sign up <a href="/signup">here.</a></p>
    </form>
  </body>
</html>
